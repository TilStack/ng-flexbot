{
  "name": "ng-flexbot",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ng-flexbot/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ng-flexbot/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "inputs": ["production", "^production"],
      "outputs": ["{workspaceRoot}/dist/libs/ng-flexbot"],
      "dependsOn": ["^build"],
      "executor": "@nx/js:tsc",
      "options": {
        "project": "packages/ng-flexbot/package.json",
        "outputPath": "packages/ng-flexbot/dist",
        "main": "packages/ng-flexbot/src/index.ts",
        "tsConfig": "packages/ng-flexbot/tsconfig.lib.json",
        "assets": []
      }
    }
  }
}
